{
  "name": "case-converter",
  "version": "0.1.1",
  "description": "transform object to use different case conventions",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "test": "babel-node node_modules/.bin/babel-istanbul cover _mocha test/*.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Moezalez@github.com/Moezalez/case-converter.git"
  },
