{
  "name": "case-converter",
  "description": "transform object to use different case conventions",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov token=$CODECOV_TOKEN",
    "build": "webpack",
    "prebuild": "rm -rf dist",
    "watch:test": "npm test -- -w",
    "test": "NODE_ENV=test mocha src/**.test.js* --compilers js:babel-register",
    "cover": "nyc  --reporter=lcov --reporter=text npm t",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Moezalez/case-converter.git"
  },
  "keywords": [
    "camelCase",
    "snake_case",
    "camel",
    "snake",
    "case",
    "converter"
  ],
  "author": "Moritz Sattler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Moezalez/case-converter/issues"
  },
  "homepage": "https://github.com/Moezalez/case-converter#readme",
  "tonicExampleFilename": "example.js",
  "devDependencies": {
    "babel-loader": "6.2.4",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.9.0",
    "codecov": "1.0.1",
    "commitizen": "2.8.2",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.2.4",
    "mocha": "2.5.3",
    "nyc": "6.6.1",
    "semantic-release": "^4.3.5",
    "standard": "7.1.2",
    "webpack": "1.13.1"
  },
  "dependencies": {
    "lodash.camelcase": "^4.1.1",
    "lodash.kebabcase": "^4.0.1",
    "lodash.snakecase": "^4.0.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "__coverage__"
        ]
      }
    }
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".es6"
    ],
    "exclude": [
      "src/**/*.test.jsx",
      "src/**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
