{
  "name": "case-converter",
  "version": "0.0.0-semantically-released",
  "description": "transform object to use different case conventions",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "test": "babel-node node_modules/.bin/babel-istanbul cover _mocha test/*.js",
    "ciTest": "babel-node node_modules/.bin/babel-istanbul cover _mocha test/*.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Moezalez/case-converter.git"
  },
  "keywords": [
    "camelCase",
    "snake_case",
    "camel",
    "snake",
    "case",
    "converter"
  ],
  "author": "Moritz Sattler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Moezalez/case-converter/issues"
  },
  "homepage": "https://github.com/Moezalez/case-converter#readme",
  "tonicExampleFilename": "example.js",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-istanbul": "^0.8.0",
    "babel-preset-es2015": "^6.9.0",
    "commitizen": "2.8.2",
    "coveralls": "^2.11.9",
    "cz-conventional-changelog": "1.1.6",
    "mocha": "^2.5.3",
    "semantic-release": "^4.3.5",
    "standard": "^7.1.2"
  },
  "dependencies": {
    "lodash.camelcase": "^4.1.1",
    "lodash.kebabcase": "^4.0.1",
    "lodash.snakecase": "^4.0.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
